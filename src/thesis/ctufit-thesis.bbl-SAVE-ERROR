      \strng{namehash}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{fullhash}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{fullhashraw}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{bibnamehash}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{authorbibnamehash}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{authornamehash}{1789aa1c9288bf72c70b1deecd9d7d11}
      \strng{authorfullhash}{1789aa1c9288bf72c70b1deecd9d7d11}
