      \strng{editorbibnamehash}{7c3dda83a9d34ca480a8d30a423f8916}
      \strng{editornamehash}{7c3dda83a9d34ca480a8d30a423f8916}
      \strng{editorfullhash}{7c3dda83a9d34ca480a8d30a423f8916}
