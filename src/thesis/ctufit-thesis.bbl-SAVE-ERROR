    \entry{NasukawaSA}{inproceedings}{}{}
      \name{author}{2}{}{%
        {{hash=b066cbbffb1f3e37e6f6c4fb0d7fd16b}{%
           family={Nasukawa},
           familyi={N\bibinitperiod},
